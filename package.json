{
	"name": "wordwrap-status",
	"displayName": "Wordwrap status",
	"description": "Shows the current wordwrap status and allows it to be changed without affecting the stored `settings`",
	"version": "0.1.7",
	"keywords": [
		"wordwrap"
	],
	"publisher": "grahammkelly",
	"private": true,
	"license": "GPL3",
	"repository": {
		"type": "git",
		"url": "https://github.com/grahammkelly/wrapstatus-vscode-extension"
	},
	"bugs": {
		"url": "https://github.com/grahammkelly/wrapstatus-vscode-extension/issues"
	},
	"engines": {
		"vscode": "^1.97.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^22.15.21",
		"@types/vscode": "^1.99.1",
		"@typescript-eslint/eslint-plugin": "^5.45.1",
		"@typescript-eslint/parser": "^5.45.1",
		"eslint": "^9.25.1",
		"typescript": "^5.8.2"
	}
}
